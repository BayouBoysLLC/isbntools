#!/usr/bin/env python
# -*- coding: utf-8 -*-

import sys
from isbntools import quiet_errors
from isbntools.ext import doi


def usage():
    print('Usage: isbn_doi ISBN')


if __name__ == "__main__":
    sys.excepthook = quiet_errors
    try:
        print(doi(sys.argv[1]))
    except:
        usage()
